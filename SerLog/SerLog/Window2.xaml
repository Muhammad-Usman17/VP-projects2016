<Window x:Class="SerLog.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SerLog"
        mc:Ignorable="d"
       Title="Setting" Height="350" Width="525"
    WindowStartupLocation="CenterScreen" WindowState="Maximized" Foreground="#FFFDFDFD" Background="#FFD7E9F9">
    <Viewbox Stretch="Uniform" StretchDirection="Both">
        <Grid>
            <Label x:Name="label" Content="Email Address" HorizontalAlignment="Left" Margin="116,93,0,0" VerticalAlignment="Top" Width="89"/>
            <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="214,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" TextChanged="textBox_TextChanged"/>
            <Button x:Name="button" Content="Save " HorizontalAlignment="Left" Margin="250,130,0,104" Width="51" Click="button_Click"/>
            <Label x:Name="label1" Content="Change Setting" HorizontalAlignment="Left" Margin="207,2,0,0" VerticalAlignment="Top" Width="94"/>
            <Button x:Name="button1" Content="Back" HorizontalAlignment="Left" Margin="1,6,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
            <Button x:Name="button2" Content="Test" HorizontalAlignment="Left" Margin="253,166,0,0" VerticalAlignment="Top" Width="51" Height="20" Click="button2_Click_1"/>

        </Grid>
    </Viewbox>
</Window>
